{
    "name": "ecigdis/intelligence-hub",
    "description": "CIS Knowledge Base Intelligence Management System",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "nikic/php-parser": "^5.6",
        "symfony/finder": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CIS\\Intelligence\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
